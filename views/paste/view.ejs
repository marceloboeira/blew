<div class="container"> 
	<div class="row">
		<div class="container ">
    		<h1><i class="fa fa-file-code-o "></i> <%= paste.name %></h1>
    		<hr>
			<i class="fa fa-code"></i> <%= paste.language %> |
			<i class="fa fa-user"></i> <a pjax href="<%= paste.owner.getLink() %>"><%= paste.owner.name %> </a> |
			<i class="fa fa-calendar"></i> <%= __("Created") %><%-: paste.createdAt | dateComponent %> |
			<i class="fa fa-clock-o"></i> <%= __("Updated") %><%-: paste.createdAt | dateComponent %>
			<hr>
    	</div>
    </div>
	<div class="row">
		<div class="container"> 
			<pre><code highlight class="<%= paste.language %>"><%= paste.content %></code></pre>
		</div>
	</div>
</div> 
