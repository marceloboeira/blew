<div class="container"> 
	<div class="row">
		<div class="container ">
    		<h1><i class="fa fa-file-code-o "></i> <%= paste.name %></h1>
    		<hr>
			<i class="fa fa-code "></i> <%= paste.language %> |
			<i class="fa fa-user "></i> <a href="<%= paste.owner.toJSON().link %>"><%= paste.owner.name %> </a> |
			<i class="fa fa-calendar "></i> <%= paste.createdAt %> |
			<i class="fa fa-calendar "></i> <%= paste.updatedAt %> 
			<hr>
    	</div>
    </div>
	<div class="row">
		<div class="container"> 
			<pre><code class="<%= paste.language %>"><%= paste.content %></code></pre>
		</div>
	</div>
</div> 
